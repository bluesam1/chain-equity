# Chain Equity Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit .env files with actual secrets

# Supabase Configuration
# Get these values from your Supabase project dashboard: https://app.supabase.com
SUPABASE_URL=your-supabase-project-url
SUPABASE_ANON_KEY=your-supabase-anon-key

# Blockchain Configuration
# For local development, use Hardhat's default RPC URL
# For production, use your preferred RPC provider (Infura, Alchemy, etc.)
RPC_URL=http://localhost:8545

# Wallet Private Key
# Used for contract deployment and backend operations
# NEVER commit this value - keep it secure
# Generate a new wallet for development or use a test account
PRIVATE_KEY=your-wallet-private-key

# Contract Address
# Deployed contract address (will be populated after contract deployment)
# This should be the same address used by both frontend and backend
CONTRACT_ADDRESS=
